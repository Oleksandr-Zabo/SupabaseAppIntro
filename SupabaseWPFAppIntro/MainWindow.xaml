<Window x:Class="SupabaseWPFAppIntro.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SupabaseWPFAppIntro"
        mc:Ignorable="d"
        Title="School Desktop App" Height="450" Width="800" MinWidth="630" MinHeight="430"
        WindowStartupLocation="CenterScreen" Icon="Assets/Icon/logo.ico"
        WindowState="Maximized" 
        Loaded="MainWindow_OnLoaded">
    <Window.Background>
        <ImageBrush  ImageSource="../Assets/Images/loading.png"/>
    </Window.Background>
    <Grid>
            <Frame x:Name="MainWindowFrame" NavigationUIVisibility="Hidden"></Frame>
            
        </Grid>
</Window>
